/*ys-cell*/
.ys-cell,.list-item{
  display: flex;
  position:relative;
  padding:0 30rpx;
  height:106rpx;
  align-items: center;
  justify-content:flex-start;
}
.ys-cell:before,
.list-item:before{
  display: block;
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #eee;
  color: #D8D8D8;

}
.ys-cell:first-child:before,
.list-item:first-child:before{
  display:none;
}

.ys-cell .ys-cell-label{
  position:relative;
  width:200rpx;
  padding-right:30rpx;
  font-size:30rpx;
  color:#333;
}
.ys-cell.request .ys-cell-label:after{
  display: inline-block;
  position: relative;
  margin-left:10rpx;
  content:"*";
  color:red;
  transform: translateY(-10%);
}

.ys-cell .ys-cell-content{
  flex:1;
  display: inline-flex;
  justify-content: flex-end;
  font-size:28rpx;
  color:#999;
}

.ys-cell .ys-cell-content input{
  text-align: right;

}
