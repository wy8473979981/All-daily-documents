.ys-block{
  position: relative;
  height:100%;
}
.ys-block-gap{
  border-top:solid 30rpx #f5f5f5;
}
.ys-block-gap-top{
  padding: 30rpx 25rpx;
}
.ys-block-gap-side{
  padding: 0rpx 25rpx;
}
.ys-block-fixed-header{
  padding-top:88rpx;
}
.ys-block-fixed-footer{
  padding-bottom:99rpx;
}
.ys-section{
  display: block;
  position:relative
}
.ys-section-gap{
  border-top:solid 30rpx #f5f5f5;
}
.section-separate{
  padding:0 30rpx;
  height:72rpx;
  line-height: 72rpx;
  background-color:#f5f5f5;
}
.section-separate-title{
  font-size:24rpx;
  color:#999;
}


.ys-section .ys-section-title{
  padding:30rpx;
  font-size:30rpx;
  color:#333;
}
.ys-section .ys-section-title text{
  margin-left: 10rpx;
  color: #bbbb;
  font-size: 28rpx;
}
.ys-section .ys-section-content{
  padding:30rpx;
}
.ys-section .ys-section-title+.ys-section-content{
  padding-top:0;
}

.ys-section .ys-section-content.ys-section-no-padding{
  padding:0;
}

.text-red{
  color:#d8004b!important;
}

/*ys-thumbnail-wrapper*/
.ys-thumbnail-wrapper{
   display:grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    grid-gap: 20rpx;
    justify-content:space-between;
    align-content:start;
    align-items:start;
    justify-items:start;
}

.ys-thumbnail-wrapper .thumbnail{
    position: relative;
    height:128rpx;
    width:128rpx;
}
.ys-thumbnail-wrapper .thumbnail image{
  width:100%;
  height:100%;
}
.ys-thumbnail-wrapper .thumbnail .thumbnail-del{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width:40rpx;
  height:40rpx;
}

.ys-thumbnail-wrapper .thumbnail.thumbnail-add{
  line-height: 124rpx;
  text-align: center;
  font-size:56rpx;
  color: #e0e0e0;
  border: dashed 2rpx #bbb;
  border-radius: 10rpx;
}
.ys-fixed-header{
    position: fixed;
    width:100%;
    height:80px;
    background-color:#fff;
    top:0;
    left:0;
}


.ys-fixed-footer{
    position: fixed;
    display: flex;
    z-index: 2;
    width:100%;
    height:99rpx;
    border-top:solid 1rpx #eee;
    background-color:#fff;
    bottom:0;
    left:0;
}
.ys-fixed-footer .form-id button{
  position: fixed;
  opacity: 0;
  fill-opacity: 0;
  z-index: 999;
  height: 99rpx;
  left: 0;
  right: 0;
  bottom: 0;
}
.ys-nav-item {
  /* padding: 30rpx 25rpx; */
  height: 104rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.ys-nav-item:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #eee;
  color: #D8D8D8;
}
.ys-nav-item:first-child:before {
  display: none;
}
.ys-nav-item .ys-icon{
  display: block;
  margin: 0 20rpx;
  width:44rpx;
  height:44rpx;
}

.nav-text {
  flex: 1;
  font-size:32rpx;
  color:#333;
}

.nav-arrow {
  padding-right: 26rpx;
  position: relative;
}
.nav-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}

/*.ys-figure*/
.ys-figure-wrapper{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

  width:100%;
  height:360rpx;
}
.ys-figure {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.ys-figure-horizontal{
    display: flex;
   flex-direction: row;
}
.ys-figure-horizontal .ys-figure-info{
    flex:1;
}

.ys-figure-avatar {
  width: 124rpx;
  height: 124rpx;
  margin: 26rpx;
  border-radius: 50%;
  overflow:hidden;

}
.ys-figure-avatar image{
    width:100%;
    height:100%;
    object-fit: cover;
}

.ys-figure-info {
  color: #aaa;
  font-size:34rpx;
}
.ys-figure-info .ys-info-title,
.ys-figure-info .ys-info-detail{
  display: flex;
  justify-content: start;
}
.ys-figure-info .ys-info-title{
  font-size:34rpx;
}
.ys-figure-info .ys-info-detail{
  font-size:24rpx;
}
/*ys-icon*/
.ys-icon{
  display: inline-block;
  width:32rpx;
  height:32rpx;
  background-repeat: no-repeat;
  background-size:contain;
  background-position: center center;
}

.ys-btn{
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius:0;
  text-align:center;
  font-size:32rpx;
}
.ys-btn:before,
.ys-btn:after{display: none;}

.ys-fixed-footer .ys-btn{
  height:100%;
  border:0;
  border-radius:0;
  flex-grow:1;
  flex-shrink: 1;
}
.ys-fixed-footer .ys-btn .ys-icon{
  margin-right:20rpx;
}


.ys-btn-powderblue{ 
  background-color:#02d6c0;
  color:#fff;
}
.ys-btn-powderblue-reverse{
  color:#02d6c0;
  background-color:#fff;
  border:solid 4rpx #02d6c0;
}
/*ys-badge*/
.ys-badge{
  display: inline-block;
  height:28rpx;
  padding:4rpx 8rpx;
  background-color:#95cbef;
  color:#fff;
