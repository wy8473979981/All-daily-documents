/* ys-modal */
.ys-modal {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1050;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
    align-items: center;
    justify-content: center;
}

.ys-modal:after{
  display: block;
  position: absolute;
  top:0;
  left:0;
  bottom:0;
  right:0;
  background-color:rgba(33,33,33,.18);
  content:" ";
  z-index:9;
}

.ys-modal.show{
  display: flex;
}
.ys-modal-dialog {
    display: flex;
   flex-direction: column;
    width: 86%;
   
    z-index:999;
}

.ys-modal-content {
    position: relative;
     min-height:280rpx;
    background-color: #fff;
    
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border:0;
    border-radius: 6px;
    outline: 0;
    -webkit-box-shadow: 0 3px 9px rgba(0,0,0,.22);
    box-shadow: 0 3px 9px rgba(0,0,0,.22);
}

/*ys-swiper-tab*/
.ys-fixed-header{
   position:fixed;
   top:0;
   left:0;
   width:100%;
   height:88rpx;
   z-index:2999;
}

.ys-swiper-tab{
  display:flex;
  flex-direction: row;

  justify-content: space-around;  

}
.swiper-tab-item{
  flex-grow:0;
  flex-shrink: 1;
  display: flex;
  position: relative;
  justify-content: center;
  align-items: center;

  font-size:28rpx;
}
.ys-tab-col-2 .swiper-tab-item{
  flex-basis: 50%;
}
.swiper-tab-item + .swiper-tab-item:after{
  position: absolute;
  display: block;
  left:0;
  top:50%;
  height:44rpx;
  width:2rpx;
  background-color:#ccc;
  transform:translateY(-50%);
  content:" ";
}

.swiper-tab-item .tab-nav{
  display: block;
  min-width:160rpx;
  line-height:84rpx;
  text-align: center;
  padding:0 20rpx;
  height:84rpx;
  border-bottom:solid 4rpx transparent;
  color:#666;

}
.swiper-tab-item.active .tab-nav{
  color:#02d6c0;
  border-color:#02d6c0;
}
.select-person{
