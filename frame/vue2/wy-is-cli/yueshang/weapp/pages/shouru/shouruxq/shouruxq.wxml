<view class="shouru-zujin">
  <app-header>
    <app-project-selector bindselected="onProjectSelected" selected='{{selected}}' disabled-all/>
    <view class="" wx:if='{{params.bisProjectId}}'>
    </view>
    <view slot="right" class='slot-right'>
      <app-picker  bind:changed="onDateChanged"   fields="year" fromDate='{{params.year}}'/>
      <view class="header-right-select">
				<app-filter-dialog groups="{{ groups }}" bindselected="bindselected">
					<view class="select-indicator">筛选</view>
				</app-filter-dialog>
			</view>
    </view>
</app-header>
	<!-- <view class="header">
		<view class=""> {{ projectName }} </view>
		<view class="header-right-box">
			<view class="header-right-time"> 2020</view>
			<view class="header-right-select">
				<app-filter-dialog groups="{{ groups }}" bindselected="bindselected">
					<view class="select-indicator">筛选</view>
				</app-filter-dialog>
			</view>
		</view>
	</view> -->
	<view class="tab-list-box">
		<view
			class="tab-list-item {{ item.type === params.type ? 'border-current' : '' }}"
			data-type="{{ item.type }}"
			wx:for="{{ tabList }}"
			wx:key="index"
			bindtap="changeTab"
		>
			{{ item.label }}
		</view>
	</view>
	<app-section title="收缴率" collapseable="{{ true }}">
		<view class="registered-channels">
			<app-echarts options="{{page.charts.main.options}}"></app-echarts>
		</view>
	 </app-section>
	<app-section title="本年收入">
		<view slot="head-actions">
			<view class="select-indicator">
				<view class="mount {{ timeSpan === '0' ? 'active' : '' }}" bindtap="changeSpan" data-span="0"> 当月 </view>
				<view class="line"> | </view>
				<view class="year {{ timeSpan === '1' ? 'active' : '' }}" bindtap="changeSpan" data-span="1"> 年累计 </view>
			</view>
		</view>
		<app-table
			totalRow="{{ totalRow }}"
			values="{{ dataList }}"
			columns="{{ columns }}"
		></app-table>
	</app-section>
</view>
