<view><block wx:if="{{globalBisProjectId}}"><view class="app-project-selector"><view>{{globalProjectName}}</view><block wx:if="{{bisProjectId}}"><view data-event-opts="{{[['tap',[['showLeaderDetail',['$event']]]]]}}" class="project-leader" catchtap="__e"><image class="leader-avatar" src="/static/assets/images/leader.png" lazy-load="false"></image><view>项目总</view></view></block></view></block><block wx:else><view data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" class="app-project-selector" bindtap="__e"><view>{{component.selected.label}}</view><block wx:if="{{!disabled}}"><image class="app-project-selector-icon" src="/static/assets/images/icon-selector-arrow.png"></image></block><block wx:if="{{bisProjectId}}"><view data-event-opts="{{[['tap',[['showLeaderDetail',['$event']]]]]}}" class="project-leader" catchtap="__e"><image class="leader-avatar" src="/static/assets/images/leader.png" lazy-load="false"></image><view>项目总</view></view></block></view></block><block wx:if="{{showLeader}}"><cover-view class="project-leader-modal" catchtouchmove="true"><cover-view class="leader-modal-inner"><block wx:if="{{leaderDetail}}"><cover-view class="leader-modal-inner-content"><cover-view class="leader-name">{{leaderDetail.userName}}</cover-view><block wx:for="{{leaderDetail.resultList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cover-view class="leader-title">{{''+item.workDuty+" ("+(item.type===1?item.startDate:item.startDate+"-"+item.endDate)+')'}}</cover-view></block><cover-view class="leader-title">{{"入职："+leaderDetail.attendWorkDate}}</cover-view></cover-view></block><cover-view data-event-opts="{{[['tap',[['closeModal',['$event']]]]]}}" class="leader-close" bindtap="__e">X</cover-view></cover-view></cover-view></block></view>