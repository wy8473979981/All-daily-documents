\n * @param {Object} params [请求时携带的参数]\r\n */\r\nexport function get(url, params) {\r\n\treturn new Promise((resolve, reject) => {\r\n\t\taxios\r\n\t\t\t.get(url, {\r\n\t\t\t\tparams: params,\r\n\t\t\t})\r\n\t\t\t.then((res) => {\r\n\t\t\t\tresolve(res.data);\r\n\t\t\t})\r\n\t\t\t.catch((err) => {\r\n\t\t\t\treject(err.data);\r\n\t\t\t});\r\n\t});\r\n}\r\n"]}]}